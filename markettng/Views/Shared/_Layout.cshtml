


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - markettng</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/sweetalert2/sweetalert2.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/toastr/toastr.min.css" asp-append-version="true" />


    <style>
        .btn{
            color: black !important;
            font-family: Noto Naskh !important;
            font-size:14px !important;
            font-weight:bold !important;
            background-color: #50b3c7 !important;
        }
        .pargger {
            height: 3rem;
            width: 4rem;
        }

        .route-title {
            color: black !important;
            font-family: Noto Naskh;
        }
        form{
            direction:rtl;
        }
    </style>
</head>

<body id="body">

    @if (ViewContext.HttpContext.User.IsInRole("admin") || ViewContext.HttpContext.User.IsInRole("manager") || ViewContext.HttpContext.User.IsInRole("user"))
{
   

        <div class="side-bar">
            <!-- add class "side-bar-light" here for light version -->
            <nav class="navbar navbar-inverse">

                <!-- toogle button -->
                <button class="navbar-toggler hidden-lg-up" type="button" id="toggler" aria-controls="list-container" aria-label="Toggle side-bar">
                    <span class="swal2-icon-show">
                        <div class=" bRMDJf islir" jsname="Q4LuWd">
                            <img class="pargger" src="data:image/png;base64," data-deferred="1" class="rg_i Q4LuWd" jsname="blue" alt="SVG Hamburger Menu | CSS-Tricks - CSS-Tricks" data-iml="1162.5" data-atf="true">
                        </div>
                    </span>
                </button>
                <!-- brand name goes below -->

                <a class="navbar-brand route route-title" value="../Home/Home">كوانتم</a>
                <div class="list-container" id="show-drawer" show="true">
                    <ul>
                        <!-- list of side bar elements goes here,-->
                        @if (ViewContext.HttpContext.User.IsInRole("admin"))
                        {
                            <li><a class="route route-title" value="../Account/Index"> <i class="fa fa-sticky-note"></i> <span class="navIcon">المستخدمين</span></a></li>
                            <li><a class="route route-title" value="../RolesManager/Index"><span class="navIcon"><i class="fa fa-star-half-o"></i></span><span>الصلاحيات</span></a></li>
                            <li><a class="route route-title" value="../Reports/Index"><span class="navIcon"><i class="fa fa-star-half-o"></i></span><span>التقارير</span></a></li>
                        }
                        @if (ViewContext.HttpContext.User.IsInRole("admin") || ViewContext.HttpContext.User.IsInRole("manager") || ViewContext.HttpContext.User.IsInRole("user"))
                        {
                            <li><a class="route" value="../Facility/Index"><span class="navIcon"><i class="fa fa-star-half-o"></i></span><span>الزيارة</span></a></li>
                        }
                        @if (ViewContext.HttpContext.User.IsInRole("admin") || ViewContext.HttpContext.User.IsInRole("manager"))
                        {
                            <li><a class="route route-title" value="../Department/Index"><span class="navIcon"><i class="fa fa-star-half-o"></i></span><span>الخدمات</span></a></li>
                            <li><a class="route route-title" value="../FacilityType/Index"><span class="navIcon"><i class="fa fa-star-half-o"></i></span><span>انواع المنشأت</span></a></li>

                        }




                        @* <li><a class="route" value="../OrderResult/Index"><span class="navIcon"><i class="pers "></i></span><span>OrderResult</span></a></li>*@
                        <li><a class="route-title" asp-controller="UserAccess" asp-action="Logout"><span class="navIcon"><i class="pers "></i></span><span>تسجيل الخروج</span></a></li>

                    </ul>
                </div>
            </nav>
        </div>
        <div class="container">
            <span id="error" style="color:red"> </span>
            <main role="main" class="pb-3" >
                <div class="content-wrapper" id="main-page">

                    @RenderBody()
                </div>
            </main>
        </div>


        @*<footer class="border-top footer text-muted">
    <div class="container">
    &copy; 2023 - markettng - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>*@
        @*  </footer>*@
        <script defer src="~/lib/sweetalert2/sweetalert2.js"></script>
        <script defer src="~/lib/jquery/dist/jquery.min.js"></script>
        <script defer defer src="~/lib/toastr/toastr.min.js"></script>
        <script defer src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

        <script defer type="module" src="~/js/routting.js" asp-append-version="true"></script>
        <script defer type="module"  src="~/js/site.js" asp-append-version="true"></script>

        <script defer type="module" src="~/js/post.js" asp-append-version="true"></script>
        <script defer type="module" src="~/js/Edit.js" asp-append-version="true"></script>
        <script defer type="module" src="~/js/delete.js" asp-append-version="true"></script>
      
        @await RenderSectionAsync("Scripts", required: false)

    
}
else
    {
      
    <div class="container">
            <span id="error" style="color:red"></span>
        <main role="main" class="pb-3">
            <div class="content-wrapper">
                    @RenderBody()
                    <div class="alert alert-warning text-center"><strong>يجب عليك مراجعة المدير لكي يتم تنشيط حسابك </strong>
                        <a class="route-title" asp-controller="UserAccess" asp-action="Logout"><span class="navIcon"><i class="pers "></i></span><span>تسجيل الدخول</span></a>
                    </div>
            </div>
        </main>
    </div>
   
}
    </body>
</html>
